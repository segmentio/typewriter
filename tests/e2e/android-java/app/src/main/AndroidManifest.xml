<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.segment.TypewriterJavaExample">

  <uses-permission android:name="android.permission.INTERNET" />

  <application
      android:name=".TypewriterApplication"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name="com.segment.TypewriterJavaExample.DisplayMessageActivity"
        android:parentActivityName="com.segment.TypewriterJavaExample.MainActivity">
        <!-- The meta-data tag is required if you support API level 15 and lower -->
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.segment.TypewriterJavaExample.MainActivity" />
    </activity>
  </application>

</manifest>
