{
  "name": "typewriter-node-javascript",
  "version": "1.0.0",
  "description": "Example app showcasing Typewriter in Node.js with JavaScript",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "DEBUG=node:* yarn start",
    "typewriter-remote": "typewriter gen-js --trackingPlanId {YOUR_TRACKING_PLAN_ID} --workspaceSlug {YOUR_WORKSPACE_SLUG} --token {YOUR_API_TOKEN} --outputPath ./generated --module CommonJS --target ES2017",
    "typewriter-local": "typewriter gen-js --inputPath ../local-tracking-plans/tracking-plan-mobile.json --outputPath ./generated --module CommonJS --target ES2017",
    "generate": "yarn run typewriter-local"
  },
  "dependencies": {
    "analytics-node": "^3.3.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "typewriter": "^3.1.3"
  }
}
