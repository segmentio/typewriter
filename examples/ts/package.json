{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run typewriter-local && next",
    "build": "npm run typewriter-local && next build",
    "start": "next start",
    "typewriter-remote": "typewriter gen-ts --trackingPlanId {YOUR_TRACKING_PLAN_ID} --workspaceSlug {YOUR_WORKSPACE_SLUG} --token {YOUR_API_TOKEN} --outputPath ./pages/generated",
    "typewriter-local": "typewriter gen-ts --inputPath ../local-tracking-plans/tracking-plan-web.json --outputPath ./pages/generated",
    "generate": "yarn run typewriter-local"
  },
  "dependencies": {
    "@zeit/next-typescript": "1.1.0",
    "next": "^7.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "request": "^2.88.0",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@types/next": "^7.0.1",
    "@types/react": "^16.0.36",
    "@types/styled-jsx": "^2.2.3",
    "fork-ts-checker-webpack-plugin": "^0.4.3",
    "typewriter": "1.0.0"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "next/babel",
      "@zeit/next-typescript/babel"
    ]
  }
}
